{
  "name": "vietnamese-days",
  "version": "1.5.4",
  "description": "Ngày lễ Việt Nam, ngày nghỉ bù, ngày làm việc, âm lịch dương lịch, hỗ trợ TS, CommonJS, UMD, định dạng ics cho Google Calendar, Apple Calendar, Microsoft Outlook...",
  "main": "dist/index.min.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrtinhnguyen/vietnamese-days.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "LICENSE",
    "package.json",
    "README.md",
    "README.en.md"
  ],
  "scripts": {
    "build": "tsc && vite build && ts-node scripts/build-json.ts && ts-node scripts/build-ics.ts",
    "fetch": "ts-node scripts/fetch.ts",
    "auto-update": "ts-node scripts/auto-update-holidays.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint:check": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:lint": "npm run format && npm run lint"
  },
  "keywords": [
    "vietnamese-date",
    "holiday",
    "workday",
    "rest day",
    "24 solar terms",
    "lunar calendar"
  ],
  "author": "Tinh Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrtinhnguyen/vietnamese-days/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.7.5",
    "cheerio": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "ical-generator": "^7.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3"
  }
}
